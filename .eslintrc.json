{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "keyword-spacing": "error",
    "linebreak-style": "error",
    "quotes": ["error", "single"],
    "semi": "error",
    "space-before-blocks": "error",
    "space-before-function-paren": "error",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
