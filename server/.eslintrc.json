{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","plugin:@typescript-eslint/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "keyword-spacing": "error",
      "linebreak-style": "error",
      "quotes": ["error", "single"],
      "semi": "error",
      "space-before-blocks": "error",
      "space-before-function-paren": "error",
      "no-undef": "off",
      "no-unused-vars": "off"
    }
}
